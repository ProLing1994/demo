    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "$PYTHONPATH:/home/workspace/RMAI/yuanhuan/code/demo/common"
            }
        },
        {
            "name": "(gdb) HelloWorld_linux",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/HelloWorld/HelloWorld_linux/release/app/bin/demo_hello_world",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/HelloWorld/HelloWorld_linux/release/app/bin",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}